x1=table(twiss,ss$end,x);px1=table(twiss,ss$end,px);
y1=table(twiss,ss$end,y);py1=table(twiss,ss$end,py);

twiss,betx=1,bety=1,pt=pt0,x=x0;
qx2=table(twiss,ss$end,mux);qy2=table(twiss,ss$end,muy);
x2=table(twiss,ss$end,x);px2=table(twiss,ss$end,px);
y2=table(twiss,ss$end,y);py2=table(twiss,ss$end,py);

value,qx1,qx2;
value,qy1,qy2;
value,x1,x2;
