 on0_x2=on_x2; on0_sep2=on_sep2; on0_alice=on_alice;
 on0_x5=on_x5; on0_sep5=on_sep5;
 on0_x8=on_x8; on0_sep8=on_sep8; on0_lhcb =on_lhcb;

