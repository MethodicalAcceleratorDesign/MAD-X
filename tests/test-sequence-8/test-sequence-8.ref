 kq3 = 0.3; mq3:   mq, k1:=kq3;

 kq4 = 0.3; mq4:   mq, k1:=kq4;



 ds  = 0.1;

 dds = 10;

 a = 2;

 b = 4;

 f = 4;

 g = -1;

 myseq: sequence, l=100;

 mq1, at:= 30 +b ;

 mq2, at:= 40 ;

 mq3, at:= b, from=mq2;

 mq4, at:= 60 + 4*ds+b;

 endsequence;



 beam;

