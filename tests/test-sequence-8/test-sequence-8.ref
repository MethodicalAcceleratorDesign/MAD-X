 select,  flag=twiss, class=mq, column=name,keyword,s,l,k1l;

 set,     format="12.6f";



seqedit, sequence=myseq; 

move, element=mq1, to:=10*a; 

move, element=mq2, to:=42+a; 

move, element=mq3, by:=a; 

move, element=mq4, to:=2; 

endedit;

