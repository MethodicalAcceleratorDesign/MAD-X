
! Links definitions
option, warn,info;

system,"ln -fns /afs/cern.ch/eng/lhc/optics/HLLHCV1.1 slhc";
system,"ln -fns /afs/cern.ch/eng/lhc/optics/V6.503 db5";
system,"ln -fns . result";

! Sequence definition
Option, -echo,warn,-info;

REAL CONST l.TAN = 3.7 ;REAL CONST l.TANAL = l.TAN;
REAL CONST l.TANAR = l.TAN;REAL CONST l.TANC = l.TAN;
REAL CONST l.TCT = 1.0;REAL CONST l.TCTH = l.TCT;
REAL CONST l.TCTVA = l.TCT;
call, file="db5/V6.5.seq";
call, file="slhc/hllhc_sequence.madx";

! Aperture definition

call, file="db5/aperture/aperture.b1.madx";
call, file="db5/aperture/aperture.b2.madx";
call, file="db5/aperture/aper_tol.b1.madx";
call, file="db5/aperture/aper_tol.b2.madx";
call,   file="slhc/aperture/aperture_upgrade_IT.madx";
call,   file="slhc/aperture/aperture_upgrade_MS.madx";


! Beam definition
beam, sequence=lhcb1, bv= 1, energy= 7000,  particle=proton,
  npart=2.2e+11,kbunch=2748, ex=3.35097159286e-10,ey=3.35097159286e-10;
beam, sequence=lhcb2, bv=-1, energy= 7000,  particle=proton,
  npart=2.2e+11,kbunch=2748, ex=3.35097159286e-10,ey=3.35097159286e-10;



! Strength definition

call,file="slhc/opt_round.madx";

NRJ=beam%lhcb1->energy;

! Cycle
seqedit,sequence=lhcb1;flatten;cycle,start=s.ds.l1.b1;endedit;
seqedit,sequence=lhcb2;flatten;cycle,start=s.ds.l1.b2;endedit;

! Twiss before customized knobs
set,format="22.15e";
select,flag=twiss,clear;
select,flag=twiss, pattern="IP.$",column=name,s,betx,bety,alfx,alfy,dx,dpx,dy,dpy,mux,muy,x,y,px,py;

use, sequence=lhcb1;
twiss, file=result/twiss_ipb1_flat.tfs;

use, sequence=lhcb2;
twiss, file=result/twiss_ipb2_flat.tfs;


! Knobs definitions

on_sep1:=1;on_x1:=1;phi_IR1 :=   90.000; on_sol_atlas:=0;on_crab1:=0;
on_sep5:=1;on_x5:=1;phi_IR5 :=    0.000; on_sol_cms  :=0;on_crab5:=0;
on_sep2:=1;on_x2:=1;on_alice:=1;on_sol_alice:=0;
on_sep8:=1;on_x8:=280/170;on_lhcb :=-1;
on_disp=1;

! Twiss with experiments turn off
on_alice_aux=on_alice; on_lhcb_aux=on_lhcb; on_disp_aux=on_disp;
on_alice=0; on_lhcb=0;

use, sequence=lhcb1;
twiss, file=result/twiss_ipb1_noexp.tfs;

use, sequence=lhcb2;
twiss, file=result/twiss_ipb2_noexp.tfs;

on_alice=on_alice_aux; on_lhcb=on_lhcb_aux; on_disp=on_disp_aux;

! Twiss with experiments turn on
  set,format="22.15e";
  select,flag=twiss,clear;
  select,flag=twiss, pattern="IP.$",column=name,s,betx,bety,alfx,alfy,dx,dpx,dy,dpy,mux,muy,x,y,px,py;

  use, sequence=lhcb1;
  twiss, file=result/twiss_ipb1.tfs;

  use, sequence=lhcb2;
  twiss, file=result/twiss_ipb2.tfs;


!

! Aperture calculations
if(NRJ <3499.99) { COmax:=0.004; dPmax=0.00150; VMAXI=30; SPECIF=7; };
if(NRJ >3499.99) { COmax:=0.003; dPmax=0.00086; VMAXI=30; SPECIF=7; };

! n1 method design report
emittance_norm=3.75e-6;
apbbeat=1.1;
halor=8.4; halox=7.3; haloy=7.3;
DParcx=0.273; DParcy=0.273;

! 2014-2015 updated parameters
if(NRJ >451) {
emittance_norm=3.5e-6;
apbbeat=1.1;
halor=6.0001; halox=6; haloy=6;
DParcx=0.10; DParcy=0.10;
COmax=0.002; dPmax=0.0002; VMAXI=30; SPECIF=12;
} else {
emittance_norm=3.5e-6;
apbbeat=1.05;
halor=6.0001; halox=6; haloy=6;
DParcx=0.14; DParcy=0.14;
COmax=0.004; dPmax=0.0006; VMAXI=30; SPECIF=9;
};

! Beam update for normalized emittances
beam, sequence=lhcb1, bv= 1, energy= 7000,  particle=proton,
  exn=emittance_norm, eyn=emittance_norm,
  npart=2.2e+11, kbunch=2748;
beam, sequence=lhcb2, bv=-1, energy= 7000,  particle=proton,
  exn=emittance_norm, eyn=emittance_norm,
  npart=2.2e+11, kbunch=2748;

value,COmax,dPmax,emittance_norm,apbbeat,halor,halox,haloy,DParcx,DParcy;

system, "cp db5/aperture/offset.ip*b*.tfs  .";
system, "cp slhc/aperture/offset.ip*b*.tfs .";

seqedit,sequence=lhcb1;flatten;cycle,start=s.ds.l1.b1;flatten;endedit;

use,sequence=lhcb1;
select,flag=twiss,clear;
select,flag=twiss,column=name,keyword,parent,kmax,kmin,calib,polarity,
       s,l,lrad,kick,hkick,vkick,angle,
       k0l,k1l,k2l,k3l,k4l,k5l,k0sl,k1sl,k2sl,k3sl,k4sl,k5sl,
       x,y,px,py,betx,bety,alfx,alfy,mux,muy,dx,dy,dpx,dpy,
       apertype,aper_1,aper_2,aper_3,aper_4,rtol,xtol,ytol,
      range=s.ds.l1.b1/e.ds.r1.b1;
twiss,file=result/twiss_ir1b1.tfs;

select,flag=aperture,clear;
select,flag=aperture,column=name,keyword,parent,
   s,x,y,px,py,betx,bety,alfx,alfy,mux,muy,dx,dy,dpx,dpy,
   n1,apertype,aper_1,aper_2,aper_3,aper_4,rtol,xtol,ytol,
   k0l,k1l,k2l,k3l,k4l,k5l,k0sl,k1sl,k2sl,k3sl,k4sl,k5sl;
aperture,range=s.ds.l1.b1/e.ds.r1.b1,
         offsetelem=./offset.IP1.b1.tfs,
         cor=COmax, dp=dPmax, spec=SPECIF,interval=1.0,
!         exn=emittance_norm, eyn=emittance_norm,    ! desactivate 5.02.04->5.02.05
         halo={6,halor,halox,haloy},bbeat=apbbeat,
         dparx=DParcx,dpary=DParcy,
         file=result/ap_ir1b1.tfs;

