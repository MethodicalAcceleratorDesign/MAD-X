d1_1: drift, l=1;

d1_2: drift, l=0.5;

mb: marker;

mrot: marker;

mrot2: marker;

ma: marker;



b: rbend, l=0.1, angle=30*pi/180;

q: quadrupole, l=0.5, k1=0.3;





myline: line=(d1_1,b,d1_1, mb, mrot, ma, d1_1, q, d1_2,mrot2,ma,d1_2);



beam;



